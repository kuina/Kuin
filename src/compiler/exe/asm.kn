				var addr2: bit32 :: getRefValue((asm2.a $ @ValImm).value, writer, refValueList) $ bit32
				do writer.write(\convert@bit32ToBin(addr2))
			else
				assert @isMem(asm2.a) & asm2.a.size = 8
				var tmp: int :: asm2.a.size
				do asm2.a.size :: 4
				do rex(writer, asm2.a, null)
				do asm2.a.size :: tmp
