+func getExeClassInfo(ast: \ast@AstClass): @ExeClassInfo
	if(ast.extra =& null | ast.extra.typeId <> %info)
		var info: @ExeClassInfo :: #@ExeClassInfo
		do info.typeId :: %info
		do info.addr :: @newAddr()
		do info.varSize :: 0
		do info.funcSize :: 0
		do ast.extra :: info
	end if
	ret ast.extra $ @ExeClassInfo
end func

