func dump(ns: []bit8)
	var s: []char :: ""
	for i(0, ^ns - 1)
		do s :~ " " ~ ns[i].toStr()
	end for
	do cui@print(s)
end func

func main()
	for(1, 3)
		block
			var n: int :: 123456789012
			var m: int
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< int
			do cui@print(m.toStr())
		end block

		block
			var n: float :: lib@pi
			var m: float
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< float
			do cui@print(m.toStr())
		end block

		block
			var n: char :: 'X'
			var m: char
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< char
			do cui@print(m.toStr())
		end block

		block
			var n: bool :: true
			var m: bool
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< bool
			do cui@print(m.toStr())
		end block


		block
			var n: bit8 :: 0x12b8
			var m: bit8
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< bit8
			do cui@print(m.toStr())
		end block

		block
			var n: bit16 :: 0x1234b16
			var m: bit16
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< bit16
			do cui@print(m.toStr())
		end block

		block
			var n: bit32 :: 0x12345678b32
			var m: bit32
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< bit32
			do cui@print(m.toStr())
		end block

		block
			var n: bit64 :: 0xF012345678ABCDEFb64
			var m: bit64
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< bit64
			do cui@print(m.toStr())
		end block

		block
			var n: []int :: [1, 2, 3]
			var m: []int
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< []int
			do cui@print(m[0].toStr() ~ ", " ~ m[1].toStr() ~ ", " ~ m[2].toStr())
		end block

		block
			var n: []char :: "abcdef"
			var m: []char
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< []char
			do cui@print(m)
		end block

		block
			var n: [][]char :: ["ab", "cd", "ef"]
			var m: [][]char
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< [][]char
			do cui@print(m[0] ~ ", " ~ m[1] ~ ", " ~ m[2])
		end block

		block
			var n: list<char> :: #list<char>
			var m: list<char>
			do n.add('a')
			do n.add('b')
			do n.add('c')
			var bin : []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< list<char>
			do m.head()
			while(!m.term())
				do cui@print(m.get().toStr())
				do m.next()
			end while
		end block

		block
			var n: stack<char> :: #stack<char>
			var m: stack<char>
			do n.add('a')
			do n.add('b')
			do n.add('c')
			var bin : []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< stack<char>
			while(^m <> 0)
				do cui@print(m.get().toStr())
			end while
		end block

		block
			var n: queue<char> :: #queue<char>
			var m: queue<char>
			do n.add('a')
			do n.add('b')
			do n.add('c')
			var bin : []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< queue<char>
			while(^m <> 0)
				do cui@print(m.get().toStr())
			end while
		end block

		block
			var n: dict<[]char, int> :: #dict<[]char, int>
			var m: dict<[]char, int>
			do n.add("a", 1)
			do n.add("c", 3)
			do n.add("b", 2)
			var bin : []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< dict<[]char, int>
			do cui@print(m.get("a", &).toStr() ~ ", " ~ m.get("b", &).toStr() ~ ", " ~ m.get("c", &).toStr())
		end block

		block
			enum E
				x :: 1234
			end enum

			var n: E :: %x
			var m: E
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< E
			do cui@print((m $ int).toStr())
		end block

		block
			class C()
				+var a: int
				+var b: []char
			end class

			var n: C :: #C
			var m: C :: #C
			do n.a :: 5
			do n.b :: "abc"
			var bin: []bit8 :: n $> []bit8
			do @dump(bin)
			do m :: bin $< C
			do cui@print(m.a.toStr() ~ ", " ~ m.b)
		end block
	end for
end func
