func main()
	for(1, 3)
		block
			var a: int :: 3
			var b: int :: 5
			do cui@print("\{lib@min(a, b)}")
			do cui@print("\{lib@max(a, b)}")
			var x: float :: -3.0
			var y: float :: -5.0
			do cui@print("\{lib@minFloat(x, y)}")
			do cui@print("\{lib@maxFloat(x, y)}")
		end block

		block
			do cui@print(["cc", "bb", "aa", "ee", "dd"].min())
			do cui@print(["cc", "bb", "aa", "ee", "dd"].max())
			do cui@print(["ab", "cd"].repeat(3).join("|"))
		end block

		block
			class C()
				+*func toStr(): []char
					ret "test"
				end func
			end class

			var c: C :: #C
			do cui@print("\{c}")
		end block

		block
			var l: list<char> :: #list<char>
			do l.add('c')
			do l.add('a')
			do l.add('d')
			do l.add('b')
			do l.add('e')
			do l.sort()
			do cui@print(l.toArray())
		end block

		block
			var l: list<char> :: #list<char>
			do l.add('c')
			do l.add('a')
			do l.add('d')
			do l.add('b')
			do l.add('e')
			do l.sortDesc()
			do cui@print(l.toArray())
		end block

		block
			var d: dict<char, []char> :: #dict<char, []char>
			do d.add('c', "CC")
			do d.add('a', "AA")
			do d.add('d', "DD")
			do d.add('b', "BB")
			do d.add('e', "EE")
			do cui@print(d.toArrayKey())
			do cui@print(d.toArrayValue().join(""))
		end block
	end for
end func
