func main()
	var funcs: []func<()> :: [@test1, @test2, @test3, @test4, @test5, @test6, @test7, @test8, @test9, @test10, @test11, @test12, @test13, @test14, @test15, @test16, @test17, @test18, @test19, @test20, @test21]
	for i(0, ^funcs - 1)
		do cui@print("-- Test \{i + 1} --\n")
		do funcs[i]()
	end for
end func

var s: []char :: "abc" ~ "def"

func dump(ns: []bit8)
	var s: []char :: ""
	for i(0, ^ns - 1)
		do s :~ " " ~ ns[i].toStr()
	end for
	do cui@print(s ~ "\n")
end func

